#include <stdio.h>
int main() {
int rows = 3, cols = 3;
int a[3][3] = {
       {6, 9, 8},
       {22, 1, 4},
       {7, 2, 6}
    };
int b[3][3] = {
       {4, 9, 19},
       {7, 5, 11},
       {13, 0, 1}
    };
int sum[3][3];

for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            sum[i][j] = a[i][j] + b[i][j];
        }
    }
printf("sum of the two arrays:\n");
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            printf("%d ", sum[i][j]);
        }
printf("\n");
    }
return 0;
}
